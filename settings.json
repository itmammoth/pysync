{
    //
    // pysync - settings.json
    //
    // Set destination path
    "destination": "/Volumes/HDD/backup",
    //
    // Set source paths to backup
    "sources": [
        {
            "path": "/Users/whoami/Documents"
        },
        {
            "path": "/Users/whoami/code",
            "exclusions": ["*.log", "tmp/"]
        }
    ]
}
